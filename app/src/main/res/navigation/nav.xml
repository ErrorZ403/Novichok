        <action
            android:id="@+id/action_thirdLvlFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/fourLvlFragment"
        android:name="com.example.utkiteam.App.fourLvlFragment"
        android:label="fragment_four_lvl"
        tools:layout="@layout/fragment_four_lvl">
        <action
            android:id="@+id/action_fourLvlFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/fiveFragment"
        android:name="com.example.utkiteam.App.fiveFragment"
        android:label="fragment_five"
        tools:layout="@layout/fragment_five">
        <action
            android:id="@+id/action_fiveFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/instuctionFragment"
        android:name="com.example.utkiteam.App.InstuctionFragment"
        android:label="fragment_instuction"
        tools:layout="@layout/fragment_instuction">
