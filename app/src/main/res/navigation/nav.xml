<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.utkiteam.App.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map">
        <action
            android:id="@+id/action_mapFragment_to_pgrogressFragment"
            app:destination="@id/pgrogressFragment" />
        <action
            android:id="@+id/action_mapFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_mapFragment_to_firstLvlFragment2"
            app:destination="@id/firstLvlFragment2" />
        <action
            android:id="@+id/action_mapFragment_to_secondLvlFragment"
            app:destination="@id/secondLvlFragment" />
        <action
            android:id="@+id/action_mapFragment_to_thirdLvlFragment"
            app:destination="@id/thirdLvlFragment" />
        <action
            android:id="@+id/action_mapFragment_to_fourLvlFragment"
            app:destination="@id/fourLvlFragment" />
        <action
            android:id="@+id/action_mapFragment_to_fiveFragment"
            app:destination="@id/fiveFragment" />
        <action
            android:id="@+id/action_mapFragment_self"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/pgrogressFragment"
        android:name="com.example.utkiteam.App.PgrogressFragment"
        android:label="fragment_pgrogress"
        tools:layout="@layout/fragment_pgrogress">
        <action
            android:id="@+id/action_pgrogressFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_pgrogressFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.utkiteam.App.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
