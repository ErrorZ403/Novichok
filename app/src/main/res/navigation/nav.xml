            android:id="@+id/action_instuctionFragment_to_firstLvlFragment2"
            app:destination="@id/firstLvlFragment2" />
        <action
            android:id="@+id/action_instuctionFragment_to_instuctionFragment_test_1"
            app:destination="@id/instuctionFragment_test_1" />
    </fragment>
    <fragment
        android:id="@+id/rulesWorkingFragment"
        android:name="com.example.utkiteam.App.RulesWorkingFragment"
        android:label="fragment_rules_working"
        tools:layout="@layout/fragment_rules_working">
        <action
            android:id="@+id/action_rulesWorkingFragment_to_firstLvlFragment2"
            app:destination="@id/firstLvlFragment2" />
    </fragment>
    <fragment
        android:id="@+id/rulesEmergencySituationFragment2"
        android:name="com.example.utkiteam.App.RulesEmergencySituationFragment"
        android:label="RulesEmergencySituationFragment">
        <action
            android:id="@+id/action_rulesEmergencySituationFragment2_to_firstLvlFragment2"
            app:destination="@id/firstLvlFragment2" />
    </fragment>
    <fragment
        android:id="@+id/instuctionFragment_test_1"
        android:name="com.example.utkiteam.App.instuctionFragment_test_1"
        android:label="fragment_instuction_test_1"
        tools:layout="@layout/fragment_instuction_test_1" />
</navigation>